import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        for(int i=1;i<=10;i++){
            int d = i*a;
            System.out.println(a + " " + "x" + " " +  i + " " + "=" + " " + d);
        }
    }
}
